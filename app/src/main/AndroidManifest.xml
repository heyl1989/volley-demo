<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teguholica.volleydemo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pages.JSONObjectRequestActivity"/>
        <activity android:name=".pages.JSONArrayRequestActivity"/>
        <activity android:name=".pages.StringRequestActivity"/>
        <activity android:name=".pages.POSTRequestActivity"/>
        <activity android:name=".pages.POSTURLEncodeRequestActivity"/>
        <activity android:name=".pages.HeaderRequestActivity"/>
        <activity android:name=".pages.ImageWithNetworkImageViewActivity"/>
        <activity android:name=".pages.ImageWithImageViewActivity"/>
        <activity android:name=".pages.ImageWithPlaceHolderActivity"/>
        <activity android:name=".pages.LoadFromCacheActivity"/>
        <activity android:name=".pages.InvalidateCacheActivity"/>
        <activity android:name=".pages.CancelSingleRequestActivity"/>
        <activity android:name=".pages.CancelAllRequestActivity"/>
        <activity android:name=".pages.PriorityActivity"/>
        <activity android:name=".AboutActivity"/>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="fa9d94d3d66bdb7b77423ab8e8bc3d97769a1c87" />
    </application>

</manifest>
